{
  "name": "Metronome",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "watchman watch-del-all && react-native start",
    "test": "jest",
    "adb": "adb reverse tcp:8081 tcp:8081",
    "watch_del_all": "watchman watch-del-all",
    "upgrade_all": "yarn yarn-upgrade-all",
    "podInstall": "npx pod-install",
    "pod": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd .."
  },
  "dependencies": {
    "@react-native-community/slider": "^4.4.2",
    "react": "^18.2.0",
    "react-native": "^0.72.4",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-reanimated": "^3.4.2",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-sound": "^0.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/runtime": "^7.22.10",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.2",
    "@types/react": "^18.2.20",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.2",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.0.2",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.1.6",
    "yarn-upgrade-all": "^0.7.2"
  },
  "engines": {
    "node": ">=16"
  }
}
